#include <bits/stdc++.h>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--){
        string s;
        cin>>s;
        transform(s.begin(),s.end(),s.begin(),::tolower);
        int c=0;
        for(int i=0;i<s.size();i++){
            if(s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u'){
                c=c+1;
            }
        }
        cout<<c<<"\n";
    }

    return 0;
}
